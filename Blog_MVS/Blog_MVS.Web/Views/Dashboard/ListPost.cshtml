
@{
    ViewBag.Title = "ListPost";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

@Model List<Blog_MVC.Entity.Models.Post>

<div class="col-lg-12">
    <div class="panel panel-default">


        <!-- /.panel-heading -->
        <div class="panel-body">
            <div class="table-responsive">
                <table class="table">
                    <thead>
                        <tr>
                            <th>Ad</th>
                            <th>Tarih</th>
                        </tr>
                    </thead>
                    <tbody>
                        @{
                            foreach (var item in Model)
                            {
                                <tr>
                                    <td>@item.Title</td>
                                    <td>@item.PostDate</td>
                                </tr>
                            }
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

<script type="text/javascript">

</script>